library(wooldridge)
library(stargazer)
library(car)
library(dplyr)
names(saving)
reg1 <- lm(cons~inc +educ, saving)
stargazer(reg1,  type = 'text')
pairs(saving %>% select(cons, inc, educ))
reg2 <- lm(cons~inc + educ + inc:educ, saving)
reg4 <- lm(cons~inc + educ + educ:inc, saving)
stargazer(reg2, reg4, type = 'text')
# test APE hypothesis
linearHypothesis(reg2, c("inc+ 11.58 * inc:educ "))
vif(reg2)
library(wooldridge)
library(stargazer)
