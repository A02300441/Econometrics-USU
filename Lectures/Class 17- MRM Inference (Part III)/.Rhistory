library(wooldridge)
library(stargazer)
library(dplyr)
reg <- lm(lwage~jc+univ+exper, twoyear)
summary(reg)
df          <- mutate(totcoll= jc+univ, twoyear)
reg_new     <- lm(lwage~jc+totcoll+exper, df)
summary(reg_new)
stargazer(reg, reg_new, type="text")
linearHypothesis(reg, c("jc-univ"))
