library(wooldridge)
library(stargazer)
df <- saving
head(df)
library(dplyr)
summary(df)
df <- df %>% filter(cons>0)
summary(df)
plot(df$inc, df$cons)
SRM <- reg(cons ~ inc , df)
stargazer(SRM, type = "text")
SRM <- reg(cons ~ inc , df)
SRM <- lm(cons ~ inc , df)
stargazer(SRM, type = "text")
MRM <- lm(cons ~ . , df)
stargazer(SRM,MRM, type = "text")
MRM <- lm(cons ~ inc + sav + size +educ+age+black , df)
stargazer(SRM,MRM, type = "text")
MRM <- lm(cons ~ inc  + size +educ+age+black , df)
stargazer(SRM,MRM, type = "text")
table(df$black)
stargazer(SRM,MRM, type = "text")
table(df$educ)
plot(df$inc, df$cons)
reg_base <- lm(cons~inc , df)
